{
  "name": "hebo-snapshot-repository-s3",
  "description": "Snapshot Repository implementation for hebo-js that uses AWS S3",
  "version": "2.0.0",
  "author": "Steve Caldwell <scaldwell@gmail.com>",
  "ava": {
    "failFast": true,
    "verbose": true,
    "files": [
      "test/**/*"
    ],
    "helpers": [
      "**/helpers/**/*",
      "**/fixtures/**/*"
    ]
  },
  "bugs": {
    "url": "https://github.com/stevecaldwell77/hebo-snapshot-repository-s3/issues",
    "email": "scaldwell@gmail.com"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "contributors": [
    "Steve Caldwell <scaldwell@gmail.com>"
  ],
  "dependencies": {
    "amazon-s3-uri": "^0.0.3",
    "auto-bind": "^2.1.1",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "ava": "^2.4.0",
    "aws-sdk": "^2.546.0",
    "codecov": "^3.6.1",
    "cross-env": "^6.0.3",
    "eslint": "^6.5.1",
    "eslint-config-archereducation": "^4.3.1",
    "fixpack": "^2.3.1",
    "hard-rejection": "^2.1.0",
    "hebo-validation": "^6.0.0",
    "husky": "^3.0.8",
    "lint-staged": "^9.4.2",
    "minimist": "^1.2.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "prettier-config-archereducation": "^1.0.0",
    "remark-cli": "^7.0.0",
    "remark-preset-github": "^0.0.16",
    "shortid": "^2.2.15"
  },
  "engines": {
    "node": ">=8.16.1"
  },
  "homepage": "https://github.com/stevecaldwell77/hebo-snapshot-repository-s3",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "cqrs",
    "es",
    "hebo-js"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "git add"
    ],
    "*.md": [
      "remark . -qfo",
      "git add"
    ],
    "package.json": [
      "fixpack",
      "git add"
    ]
  },
  "main": "index.js",
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100,
    "reporter": [
      "lcov",
      "html",
      "text"
    ]
  },
  "peerDependencies": {
    "aws-sdk": "^2.546.0",
    "hebo": ">=6.0.0"
  },
  "remarkConfig": {
    "plugins": [
      "preset-github"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stevecaldwell77/hebo-snapshot-repository-s3"
  },
  "scripts": {
    "ava": "cross-env NODE_ENV=test ava",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "remark . -qfo && eslint .",
    "nyc": "cross-env NODE_ENV=test nyc ava",
    "test": "npm run lint && npm run ava",
    "test-coverage": "npm run lint && npm run nyc"
  }
}
